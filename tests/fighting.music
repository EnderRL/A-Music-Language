Fragment sameNote3(Figure f1, Figure f2, Figure f3, Figure f4) {
    :f1 :f1 :f1 :f1 :f1 :f2 :f3 :f4
}

void repeat(Figure figure, int count) {
    for (int i = 0; i < count; ++i) {
        || F:sameNote(figure); ||
    }
}

Fragment firstSilence(Figure figure) {
    Silence.c F:repeatN(figure, 7);
