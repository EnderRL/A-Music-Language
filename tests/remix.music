Fragment repeatN(Figure figure, int times) {
    for (int i = 0; i < times; ++i) {
        :figure
    }
}

Fragment compas9() {
    (Fa-4 Do-4).n* ($Sol-4 Si-3).c~ (Sol-4 Si-3).n (La-4 Do-4).n~
