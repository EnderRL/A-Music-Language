Fragment compas14() {
    (La Fa Si-4).c F:repeatN((Sol $Fa Si-4).c, 7);
}

Fragment compas18() {
    (Fa Mi &Do).n* ($Do &Sol-4 Mi-4).c Silence.c (Do Sol-4 Mi-4).c (&Do Sol-4 Mi-4).n
}

Fragment sameNote2(Figure f1, Figure f2) {
